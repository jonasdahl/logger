{
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:sw": "esbuild service-worker/index.ts --bundle --outfile=public/sw.js",
    "build:prisma": "prisma generate",
    "build:remix": "remix build",
    "build:server": "tsx ./build-server.ts",
    "dev": "cross-env TZ=America/Montreal remix dev -c \"pnpm run dev:all\" --no-restart",
    "dev:all": "run-p dev:server dev:sw",
    "dev:sw": "pnpm run build:sw --watch",
    "dev:server": "cross-env TZ=America/Montreal tsx watch --clear-screen=false --ignore \"app/**\" --ignore \"build/**\" --ignore \"node_modules/**\" ./index.js",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production node .",
    "typecheck": "tsc",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "prisma": "prisma"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.6.1",
