    "@prisma/client": "^4.11.0",
    "@remix-run/express": "^1.17.0",
    "@remix-run/node": "^1.17.0",
    "@remix-run/react": "^1.17.0",
    "@remix-run/serve": "^1.17.0",
    "@remix-validated-form/with-zod": "^2.0.5",
    "@types/bcrypt": "^5.0.0",
    "@types/lodash": "^4.14.191",
    "@types/luxon": "^3.2.0",
    "@types/node": "^20.1.4",
    "@visx/xychart": "^3.1.2",
    "address": "^1.2.2",
    "aws-sdk": "^2.1012.0",
    "bcrypt": "^5.1.0",
    "chalk": "^5.2.0",
    "chokidar": "^3.5.3",
    "close-with-grace": "^1.2.0",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "framer-motion": "^10.8.4",
    "fs-extra": "^11.1.1",
    "get-port": "^6.1.2",
    "glob": "^10.2.4",
    "isbot": "^3.6.5",
    "jsdom": "^21.1.1",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "mock-aws-s3": "^4.0.2",
    "morgan": "^1.10.0",
    "nock": "^13.1.4",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.4.0",
    "remix-auth-form": "^1.3.0",
